{
  "name": "travel-and-help",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start:back": "cd src/back && nodemon",
    "start:front": "npm run build",
    "start": "npm-run-all -p start:*",
    "watch:front": "webpack-dev-server --config webpack/index.js",
    "watch:back": "npm run start:back",
    "watch": "npm-run-all -p watch:*",
    "build": "webpack --config webpack/index.js",
    "test:front": "mocha test/front --compilers js:babel-register",
    "test:back": "mocha test/back",
    "test": "npm-run-all lint coverage:*",
    "coverage:front": "nyc --report-dir=reports/coverage/front npm run test:front",
    "coverage:back": "nyc --report-dir=reports/coverage/back npm run test:back",
    "lint": "eslint src",
    "lint:front": "eslint src/front",
    "lint:back": "eslint src/back"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travel-and-help/start-kit.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/travel-and-help/start-kit/issues"
  },
  "homepage": "https://github.com/travel-and-help/start-kit#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "base64-font-loader": "0.0.3",
    "chai": "^3.5.0",
    "chalk": "^1.1.1",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "dotenv": "^2.0.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "getenv": "^0.6.0",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.14.0",
    "import-glob-loader": "^1.1.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "npm-run-all": "^1.5.3",
    "nyc": "^6.1.1",
    "postcss-loader": "^0.8.2",
    "proxyquire": "^1.7.4",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": "^5.8.0",
    "npm": "^3.8.1"
  },
  "nyc": {
    "reporter": "html",
    "exclude": [
      "**/*.spec.js",
      "test/",
      "webpack/",
      "env/",
      "reports/",
      "target/"
    ],
    "lines": 98,
    "check-coverage": true
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "react": "^0.14.7"
  }
}
